import java.util.Scanner;

public class Ex031 {

	public static void main(String[] args) {
		Scanner s= new Scanner(System.in);
		int N = s.nextInt();
		int a[] = new int[N];
		int min=0;
		int max=0;
		for(int i =0;i<N;i++) {
			a[i]=s.nextInt();
			if(max<a[i]) {
				max=a[i];
			}
			if(min>a[i]) {
				min=a[i];
			}
		}
		System.out.println(min+" "+max);
	}

}
