import java.util.Random;
import java.util.Scanner;

public class Ex7 {

	public static void main(String[] args) {
		Random r = new Random();
		Scanner s= new Scanner(System.in);
		String last[]= {"김","박","이","최","장"};
		String first[]= {"바","사","아","자","차"};
		String mid[]= {"가","나","다","라","마"};
		String fullname[]= new String[50];
		while(true) {
			System.out.println("--------------------------");
			System.out.println(" 파일 제어 프로그램 v1.1 ");
			System.out.println("--------------------------");
			System.out.println("1. 이름 생성하여 파일에 저장하기");
			System.out.println("2. 파일에서 이름 읽어오기");
			System.out.println("3. 프로그램 종료");
			System.out.println("--------------------------");
			System.out.print("메뉴 선택 : ");
			int a = s.nextInt();
			int num1 = 0;
			int num2 = 0;
			int num3 = 0;
			if (a==1) {
				for (int i=0;i<50;i++) {
					num1 = r.nextInt(5);
					num2 = r.nextInt(5);
					num3 = r.nextInt(5);
					fullname[i] = last[num1]+mid[num2]+first[num3];					
				}
			}
			for (int i=0;i<50;i++) {
				System.out.println(fullname[i]);
			}
			
			if (a==3) {
				break;
			}
		}
	}

}
