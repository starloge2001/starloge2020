
public class Ex5 {

	public static void main(String[] args) {
		int arr[][]= {{1,2,3},{4,5,6},{7,8,9}};
		int arr2[][]= {{10,20,30},{40,50,60},{70,80,90}};
		int sum[][]= new int[3][3];
		for(int i=0;i<arr.length;i++) {
			for(int j=0;j<arr.length;j++) {
				sum[i][j]=arr[i][j]+arr2[i][j];
				System.out.print(sum[i][j]+" " );
			}
			System.out.println();
		}
		
	}
}


