package ldjex;

import java.util.Scanner;

public class Ex3 {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int a = s.nextInt();
		for(int i=0;i<a;i++) {
			for(int j=0;j<a-(i+1);j++) {
				System.out.print(" ");
			}
			for(int k=0;k<a;k++) {
				System.out.print("*");				
			}
			System.out.println("");
		}
	}

}
