package ex1;

import java.util.Scanner;

public class ex5_1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner(System.in);
		double r = sc.nextInt();
		double PI = 3.14;
		double sum = r * r * PI;
		System.out.println("원의 면적 : " + sum);
	}

}
